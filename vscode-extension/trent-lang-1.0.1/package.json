{
    "name": "trentlang",
    "displayName": "TrentLang",
    "description": "Language support for Trent programming language.",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.58.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "trentlang",
            "aliases": ["Trent", "trentlang"],
            "extensions": [".trent"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "trentlang",
            "scopeName": "source.trent",
            "path": "./syntaxes/trentlang.tmLanguage.json"
        }]
    }
}