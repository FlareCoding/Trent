
func test_fn()
{
	// This is a single line comment
	for (var i = 0; i < 5; i++)
	{
		/*
			Multiline
			Comment
		*/
		// Another single line comment

		while (true)
		{
			println("inside the while loop");
			break;
		}

		println("i: " + i);
		println();
		break;
	}
}

func foo()
{
	for (var i = 1; i < 10; i++) {
		test_fn();
		println();
		
		if (i == 3) {
			break;
		}
	}

	return "foo executed";
}

var result = foo();

print("Result: ");
println(result);

