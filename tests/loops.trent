func test_fn()
{
	for (var i = 0; i < 4; i++)
	{
		while (true)
		{
			println("before return");
			return 3;
		}

		return 2;
	}

	return 1;

	println("after return statement");
}

func foo()
{
	println("foo executing...");
	return test_fn();
	println("foo after return statement??");
}

func bar()
{
	println("bar executing...");
	return foo();
	println("bar after return??");
}

var result = bar();

print("Result: ");
println(result);
