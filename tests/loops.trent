func test_fn()
{
	for (var i = 0; i < 5; i++)
	{
		while (true)
		{
			println("inside the while loop");
			break;
		}

		println("i: " + i);
		println();
		break;
	}
}

func foo()
{
	for (var i = 0; i < 10; i++) {
		test_fn();
		println();
		break;
	}

	return "foo executed";
}

var result = foo();

print("Result: ");
println(result);
