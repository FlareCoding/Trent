
func printVariableInformation(x)
{
	println("\n---- Variable Information ----");
	print  ("\tValue    : "); println(x);
	println("\tType     : " + typeof(x));
	println("\tInstance : " + instanceof(x));
}

var str = "Hello World";
var num = 24;
var num2 = 3.14;
var obj = null;

printVariableInformation(str);
printVariableInformation(num);
printVariableInformation(num2);
printVariableInformation(obj);
